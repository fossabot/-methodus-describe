<div class="col-md-8">
    <div id="accordion" role="tablist">

        <% if(routes) {%>

            <%routes.forEach(function(route){%>
                <%if(route.active){%>
                    <div class="card">
                        <div class="card-header">
                            <div class="d-flex justify-content-between">
                                <a data-toggle="collapse" class="d-flex p-1" href="#<%=cleanID(route)%>" aria-expanded="true" aria-controls="collapseOne">
                                    <h3>
                                        <%-route.name%>
                                            <small>
                                                <%=route.methodus.name%>
                                            </small>
                                    </h3>

                                </a>

                                <a class="btn btn-success">Run tests</a>
                            </div>
                        </div>
                    </div>

                    <div class="card-body collapse" id="<%=cleanID(route)%>" role="tabpanel" aria-labelledby="headingOne" data-parent="#accordion">

                        <table class="table table-striped table-hover">
                            <thead class="thead-dark">
                                <tr>
                                    <th>Test</th>
                                    <th>Verb</th>
                                    <th>Key</th>
                                    <th>Route</th>
                                    <th>Args</th>
                                </tr>
                            </thead>
                            <tbody>
                                <% Object.keys(route.methodus._descriptors).forEach(function(itemKey){%>
                                    <tr>
                                        <td>
                                            <label>
                                                <input type="checkbox" id="test_<%=route.methodus._descriptors[itemKey].propertyKey%>" />
                                            </label>
                                        </td>
                                        <td>
                                            <%=route.methodus._descriptors[itemKey].verb%>
                                        </td>
                                        <td>


                                            <a href="test/<%=route.name%>/<%=itemKey%>" target="testFrame" style="word-break: break-all;">
                                                <%=route.methodus._descriptors[itemKey].propertyKey%>
                                            </a>

                                        </td>
                                        <td>
                                            <span style="word-break: break-all;">
                                                <%=route.methodus._descriptors[itemKey].route%>
                                            </span>


                                        </td>
                                        <td>
                                            <%if(route.methodus._descriptors[itemKey].params){%>
                                               
                                                    <ul>
                                                        <% route.methodus._descriptors[itemKey].params.forEach(function(paramItem){%>
                                                            <li>
                                                                <i>
                                                                    <%=paramItem.from%>
                                                                </i>:
                                                                <strong>
                                                                    <%=paramItem.name%>
                                                                </strong> :
                                                                <u>
                                                                    <%=paramItem.type%>
                                                                </u>
                                                            </li>
                                                            <%})%>
                                                    </ul>
                                                    <%}%>
                                        </td>
                                    </tr>
                                    <% }); %>
                            </tbody>
                        </table>
                    </div>



                    <%}%>
                        <% }); %>
                            <%}%>

    </div>
    <div class="col-md-4" style="position:fixed;top:50px;right:0;">
        <iframe name="testFrame" style="width: 100%;height: 100%;" frameBorder="0"></iframe>
    </div>
</div>